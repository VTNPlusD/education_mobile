{
  "name": "education",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "format": "prettier --write .",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:eslint:fix": "eslint --ignore-path .gitignore --fix",
    "lint:ts": "yarn lint:tslint",
    "lint:tslint": "tslint --config ./tslint.json --project ./tsconfig.json",
    "lint:tslint:fix": "tslint --config ./tslint.json --project ./tsconfig.json --fix",
    "prepare": "husky install"
  },
  "eslintIgnore": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.{md,json}": [
      "prettier --write",
      "git add --force"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "npm run lint:tslint:fix",
      "git add --force"
    ],
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.13.2",
    "@react-navigation/bottom-tabs": "^5.11.11",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.5",
    "i18next": "^20.3.2",
    "lodash": "^4.17.20",
    "react": "17.0.1",
    "react-i18next": "^11.11.0",
    "react-native": "0.64.1",
    "react-native-gesture-handler": "^1.9.0",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^3.4.0",
    "react-native-svg": "^12.1.1",
    "react-redux": "^7.2.4",
    "redux-api-middleware": "^3.2.1",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/runtime": "^7.14.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/lodash": "^4.14.165",
    "@types/react": "^17.0.8",
    "@types/react-native": "^0.64.7",
    "@types/react-redux": "^7.1.16",
    "@types/redux-api-middleware": "^3.2.2",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-jest": "^27.0.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.27.0",
    "husky": "^6.0.0",
    "jest": "^27.0.3",
    "lint-staged": "^11.0.0",
    "metro-react-native-babel-preset": "^0.66.0",
    "prettier": "^2.3.1",
    "react-native-svg-transformer": "^0.14.3",
    "react-test-renderer": "17.0.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^5.0.0",
    "tslint-react-hooks": "^2.2.2",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
